				<xsd:element name="NamensVergleich" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="Success" type="xsd:boolean"/>
							<xsd:element name="NameKorrekt" type="xsd:boolean"/>
